    "type": "git",
    "url": "git+https://github.com/MichaellAlavedraMunayco/computer-scrapping.git"
  },
  "keywords": [
    "database",
    "xlsx",
    "data-visualization",
    "data-structures",
    "scrapping",
    "data-analysis",
    "powerbi",
    "export-excel",
    "scrapper-script",
    "dbdiagram"
  ],
  "author": "Michaell Alavedra Munayco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichaellAlavedraMunayco/computer-scrapping/issues"
  },
  "homepage": "https://github.com/MichaellAlavedraMunayco/computer-scrapping#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "exceljs": "^4.3.0",
    "puppeteer": "^11.0.0",
    "ts-prime": "^1.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/node": "^16.11.9",
    "@types/puppeteer": "^5.4.4",
    "tslint": "^5.20.1",
    "typescript": "^4.4.3"
  }
